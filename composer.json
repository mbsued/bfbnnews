{
    "name": "bfbnnews/bfbnnews",
    "type": "typo3-cms-extension",
    "description": "Erweiterung der Extension News um Felder fuer Termin",
    "authors": [
        {
            "name": "BFBN-Team",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^11 || ^12"
    },
    "autoload": {
        "psr-4": {
            "Bfbnnews\\Bfbnnews\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bfbnnews\\Bfbnnews\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/bfbnnews": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "bfbnnews"
        }
    }
}